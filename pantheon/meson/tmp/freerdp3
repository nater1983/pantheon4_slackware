mkdir cmake-build
cd cmake-build
  cmake .. \
    "${GNOME_CMAKE_ARGS[@]}" \
    -DRDTK_FORCE_STATIC_BUILD=ON \
    -DUWAC_FORCE_STATIC_BUILD=ON \
    -DWITH_BINARY_VERSIONING=ON \
    -DWITH_ALSA=ON \
    -DWITH_PULSE=ON \
    -DWITH_CHANNELS=ON \
    -DBUILTIN_CHANNELS=ON \
    -DWITH_CUPS=ON \
    -DWITH_PCSC=ON \
    -DWITH_JPEG=ON \
    -DWITH_FFMPEG=ON \
    -DWITH_DSP_FFMPEG=ON \
    -DWITH_WAYLAND=ON \
    -DWITH_GSSAPI=OFF \
    -DCHANNEL_URBDRC=ON \
    -DCHANNEL_URBDRC_CLIENT=ON \
    -DWITH_SERVER=ON \
    -DWITH_CAIRO=ON \
    -DBUILD_TESTING=OFF \
    -DWITH_PKCS11=OFF
make 
make install/strip DESTDIR=$PKG
